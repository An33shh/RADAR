{
  "AppConfig": {
    "ThreatFeeds": [
      {
        "Name": "AlienVault_OTX",
        "BaseUrl": "https://otx.alienvault.com/api/v1",
        "ApiKey": "",
        "RefreshInterval": "01:00:00",
        "IsActive": true,
        "RateLimitPerMinute": 60,
        "Headers": {
          "User-Agent": "RADAR-ThreatIntel/1.0"
        }
      },
      {
        "Name": "AbuseCH_MalwareBazaar",
        "BaseUrl": "https://mb-api.abuse.ch",
        "ApiKey": "",
        "RefreshInterval": "02:00:00",
        "IsActive": true,
        "RateLimitPerMinute": 30
      },
      {
        "Name": "MITRE_ATTCK",
        "BaseUrl": "https://raw.githubusercontent.com/mitre/cti/master",
        "ApiKey": "",
        "RefreshInterval": "24:00:00",
        "IsActive": true,
        "RateLimitPerMinute": 10
      }
    ],
    "MaxConcurrentRequests": 5,
    "RequestTimeout": "00:00:30",
    "LogLevel": "Information",
    "OutputDirectory": "./Reports",
    "Correlation": {
      "MinimumConfidenceThreshold": 0.7,
      "MaxIndicatorsPerCorrelation": 1000,
      "EnableInfrastructurePivots": true,
      "IgnoredDomains": [
        "google.com",
        "microsoft.com",
        "cloudflare.com",
        "amazonaws.com"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  }
}
